<?xml version="1.0" encoding="utf-8"?>
<layouts>

	<!--  	Should override internal settings so all parameters are still ok.
			Load XML into a java format with all controllers listed with x,y and all parameters
			Both LayoutManager and MidiControllerManager can use that format.
	 -->
	<defaults>
		<xypad>
			<parameter id="0" name="Press" type="controlChange" visible="true" />
			<parameter id="1" name="Horizontal" type="controlChange" visible="true" />
			<parameter id="2" name="Vertical" type="controlChange" visible="true" />
		</xypad>
		<slider>
			<parameter id="0" name="Press" type="controlChange" visible="true" />
			<parameter id="1" name="Vertical" type="controlChange" visible="true" />
		</slider>
		<pad>
			<parameter id="0" name="Press" type="note" visible="false" />
		</pad>
	</defaults>

